#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    fgets(s, sizeof(s), stdin);
    s[strcspn(s,"\n")] = '\0';

    char* start = s;
    char* end = s + strlen(s) - 1;
    int flag = 1;

    while(start < end) {
        if(*start != *end) {
            flag = 0;
            break;
        }
        start++;
        end--;
    }

    if(flag) printf("Palindrome\n");
    else printf("Not Palindrome\n");

    return 0;
}

/*
Input:
madam
Output:
Palindrome
*/
